<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.eatit">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".bill"
            android:theme="@style/log"/>
        <activity
            android:name=".menu_donalds"
            android:theme="@style/log" />
        <activity
            android:name=".registration_google"
            android:theme="@style/log" />
        <activity
            android:name=".login_main"
            android:theme="@style/log" />
        <activity
            android:name=".registration_mail"
            android:theme="@style/log" />
        <activity
            android:name=".login_mail"
            android:theme="@style/log" />
        <activity
            android:name=".Profile"
            android:theme="@style/log" />
        <activity
            android:name=".Login"
            android:theme="@style/log" />
        <activity
            android:name=".Registeration"
            android:theme="@style/log" />
        <activity
            android:name=".SplashScreen"
            android:theme="@style/SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/log" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>